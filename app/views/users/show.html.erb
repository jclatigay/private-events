<% if current_user.created_events.any? %>
    <h2>Your Created Events</h2>
        <% @created_events.each do |event| %>
            <div class="container border border-dark rounded-3 p-3">
                <p><%= link_to event.title, event_posts_path(event) %></p>
                <p><%= event.description %></p>
                <p><%= event.date %></p>
                <p><%= event.location %></p>
            </div>
        <% end %>
<% end %>