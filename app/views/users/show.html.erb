<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

<% if @created_events.any? %>
    <% @created_events.each do |event| %>
        <p><%= event.title %></p>
        <p><%= event.description %></p>
        <p><%= event.date %></p>
        <p><%= event.location %></p>
    <% end %>
<% end %>